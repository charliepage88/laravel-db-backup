{
    "name": "charliepage88/laravel-db-backup",
    "description": "Backup and restore databases. Encrypting dumps. Save dumps to db and uploading to S3. Restore dump from S3.",
    "homepage": "https://github.com/charliepage88/laravel-db-backup",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladyslav Ternal",
            "email": "wlad.monax@gmail.com"
        },
        {
            "name": "Larkin Whitaker",
            "email": "larkinwhitaker@gmail.com"
        },
        {
            "name": "Yevheniy Lypey",
            "email": "ilevgenij@gmail.com"
        },
        {
            "name": "Charles Page",
            "email": "charliepage88@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.*|^6.0",
        "aws/aws-sdk-php": "^3.112"
    },
    "autoload": {
        "psr-0": {
            "Witty\\LaravelDbBackup": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Witty\\LaravelDbBackup\\DBBackupServiceProvider"
            ]
        }
    }
}
